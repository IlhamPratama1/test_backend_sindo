RewriteEngine On # Turn on the rewriting engine
RewriteRule ^users/?$ users.php [NC,L]
RewriteRule ^users/([0-9]+)/?$ users.php?id=$1 [NC,L]
RewriteRule ^products/count?$ count.php [NC,L]
RewriteRule ^products/highest-price?$ highest.php [NC,L]
RewriteRule ^products/region?$ region.php [NC,L]
RewriteRule ^products/?$ products.php [NC,L]
RewriteRule ^products/([0-9]+)/?$ products.php?id=$1 [NC,L]
RewriteRule ^products/([^/]+)?$ products.php?page=$1 [L,QSA]
RewriteRule ^user-transaction/?$ user_transaction.php [NC,L]
RewriteRule ^user-transaction/([0-9]+)/?$ user_transaction.php?id=$1 [NC,L]